<?xml version="1.0" encoding="utf-8"?>
        <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD//EN"
                "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Developer" table="developer">
        <meta attribute="class-description">
            This class contains developer's details.
        </meta>
        <id name="id" type="int" column="idTeam">
            <generator class="native"/>
        </id>
        <property name="firstName" column="developerFirstName" type="string"/>
        <property name="teamStatus" column="developerLastName" type="string"/>
    </class>
</hibernate-mapping>
